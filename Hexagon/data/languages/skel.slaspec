	jump(tgt);
	#testCallOther(6:4, tgt);
}



macro rotl32(out, a, b) {
    	local d:4 = b;
	local n:4 = a;
    	local out1:4 = (n << d) | ( n >> (32 - d));
	out = out1;
}
macro rotl64(out, a, b) {
    	local d:8 = b;
	local n:8 = a;
    	local out1:8 = (n << d) | ( n >> (64 - d));
	out = out1;
}

