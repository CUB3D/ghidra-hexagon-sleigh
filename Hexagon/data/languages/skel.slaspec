macro jump_addr_neq(a, b, tgt) {
	if(a == b) goto <skip>;
	jump(tgt);
	<skip>
	#testCallOther(2:4, a, 1:4, b, tgt);
} 

macro jump_addr_eq(a, b, tgt) {
	if(a != b) goto <skip>;
	jump(tgt);
	<skip>
	#testCallOther(2:4, a, 0:4, b, tgt);
}


