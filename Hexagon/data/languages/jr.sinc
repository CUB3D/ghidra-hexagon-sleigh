
# JR
# JR:Call sub from reg
with slot: iclass=0b0101 {
	:"callr "S5 is imm_21_27=0b0000101 & S5 & S5i & imm_0_13=0 & mode=0 {}
	:"callr "S5 is imm_21_27=0b0000101 & S5 & S5i & imm_0_13=0 & mode=1100 {
		LR=inst_next;
		call [S5i];
	}
	
	:"callrh "S5 is imm_21_27=0b0000110 & S5 & S5i & imm_0_13=0 & mode=0 {}
	:"callrh "S5 is imm_21_27=0b0000110 & S5 & S5i & imm_0_13=0 & mode=1100 {
		LR=inst_next;
		call [S5i];
	}

	:"if ("U2_pred") callr" S5 is imm_21_27=0b0001000 & S5 & S5i & imm_10_13=0 & U2_pred & U2_predi & imm_0_7=0 & mode=0 {}
	:"if ("U2_pred") callr" S5 is imm_21_27=0b0001000 & S5 & S5i & imm_10_13=0 & U2_pred & U2_predi & imm_0_7=0 & mode=1100 {
        if(U2_predi == 0) goto <end>;
		call [S5i];
        <end>
	}
	
	:"if (!"U2_pred") callr" S5 is imm_21_27=0b0001001 & S5 & S5i & imm_10_13=0 & U2_pred & U2_predi & imm_0_7=0 & mode=0 {}
	:"if (!"U2_pred") callr" S5 is imm_21_27=0b0001001 & S5 & S5i & imm_10_13=0 & U2_pred & U2_predi & imm_0_7=0 & mode=1100 {
        if(U2_predi != 0) goto <end>;
        call [S5i];
        <end>
	}
}

# JR:Hinted call sub from reg - duped with call sub from reg

# JR:Hint indirect jmp
define pcodeop hintjr;
with slot: iclass=0b0101 & mode=0 {
	:"hintjr("S5")" is imm_21_27=0b0010101 & S5 & S5i & imm_0_13=0b00000000000000 {
		hintjr(S5i);
	}
}


